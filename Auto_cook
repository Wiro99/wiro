					noStopScript = false
					bot:disconnect()
					sleep(100)
					webhookShareInfoAllBot()
					webhookShareInfoBot(
						"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
					)
					bot:stopScript()
					removeBot(bot.name)
					sleep(1000)
				end
			elseif Break_Other_World == "NO" then
				if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
					if world:hasAccess(bot.x + 1, bot.y) == 0 then
						dropExceedPackGoOffline()
			
						while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
							seedWorld()
							dropSeed()
							while inventory:getItemCount(Block_ID) > 0 do
								dropBlock()
							end
							hiddenWorld()
						end
						
						while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
							pickaxeWorld()
							while not bot:isInTile(44, 13) do
								findpath(44, 13)
							end
							dropItem(Pickaxe_ID)
							if inventory:getItemCount(Pickaxe_ID) == 0 then
								webhookShareInfoDropped(
									"**[ BOT INFORMATION ]** \n" ..
									"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
									":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
									"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
									"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
									"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
									
									"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
									"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
									":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
									getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
								)
							end
							hiddenWorld()
						end
						
						dropEquipmentBeforeOfffline()
					
						autoRecon = false
						noStopScript = false
						bot:disconnect()
						sleep(100)
						webhookShareInfoAllBot()
						webhookShareInfoBot(
							"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
						)
						bot:stopScript()
						removeBot(bot.name)
						sleep(1000)
					elseif tileGrowScan(14142) > 0 then
						antiFire()
					end
				end
			end
		end
	end
	
	webhookShareInfoAllBot()
	
	if Auto_Retrieve == "YES" and bot.status == 1 then
		local retrieveUnstable = true
		if retrieveUnstable then
			if wrenchUnstableX == nil and wrenchUnstableY == nil then
				for _, tile in pairs(world:getTiles()) do
					if bot.status == 1 then
						if tile.fg == 6948 then
							findpath(tile.x, tile.y - 1)
							while not bot:isInTile(tile.x, tile.y - 1) do
								findpath(tile.x, tile.y - 1)
								sleep(3000)
							end
							
							wrenchUnstableX = tile.x
							wrenchUnstableY = tile.y
							break
						end
					elseif bot.status ~= 1 or (bot.status == 1 and bot:getPing() == 0) then
						defaultBotX = bot.x
						defaultBotY = bot.y
						
						if Break_Other_World == "YES" then
							if Use_Custom_World_For_Break == "YES" then
								reconnect(Custom_World_Break_Other_World, defaultBotX, defaultBotY)
							elseif Use_Random_World_For_Break_Other_Break == "YES" then
								reconnect(randomWorldBreak(), defaultBotX, defaultBotY)
							elseif Use_Tutorial_World_For_Break == "YES" then
								reconnect(tutorialWorld, defaultBotX, defaultBotY)
							end
						elseif Break_Other_World == "NO" then
							reconnect(Rotation_World[rotationWorldStartNumber]:upper(), defaultBotX, defaultBotY)
						end
					end
				end
			else
				findpath(wrenchUnstableX, wrenchUnstableY - 1)
				while not bot:isInTile(wrenchUnstableX, wrenchUnstableY - 1) do
					findpath(wrenchUnstableX, wrenchUnstableY - 1)
					sleep(3000)
				end
			end
			
			bot:setBubble(Bubble.brb)
			sleep(500)
			bot:wrench(wrenchUnstableX, wrenchUnstableY)
			addEvent(Event.variantlist, retrieveItems)
			listenEvents(5)
			bot:setBubble(Bubble.none)
		end
		
		if inventory:getItemCount(Block_ID) > 0 then
			pnb()
		end
		
		local retrieveGaia = true
		if retrieveGaia then
			if wrenchGaiaX == nil and wrenchGaiaY == nil then
				for _, tile in pairs(world:getTiles()) do
					if bot.status == 1 then
						if tile.fg == 6946 then
							findpath(tile.x, tile.y - 1)
							while not bot:isInTile(tile.x, tile.y - 1) do
								findpath(tile.x, tile.y - 1)
								sleep(3000)
							end
							
							wrenchGaiaX = tile.x
							wrenchGaiaY = tile.y
							break
						end
					elseif bot.status ~= 1 or (bot.status == 1 and bot:getPing() == 0) then
						defaultBotX = bot.x
						defaultBotY = bot.y
						
						if Break_Other_World == "YES" then
							if Use_Custom_World_For_Break == "YES" then
								reconnect(Custom_World_Break_Other_World, defaultBotX, defaultBotY)
							elseif Use_Random_World_For_Break_Other_Break == "YES" then
								reconnect(randomWorldBreak(), defaultBotX, defaultBotY)
							elseif Use_Tutorial_World_For_Break == "YES" then
								reconnect(tutorialWorld, defaultBotX, defaultBotY)
							end
						elseif Break_Other_World == "NO" then
							reconnect(Rotation_World[rotationWorldStartNumber]:upper(), defaultBotX, defaultBotY)
						end
					end
				end
			else
				findpath(wrenchGaiaX, wrenchGaiaY - 1)
				while not bot:isInTile(wrenchGaiaX, wrenchGaiaY - 1) do
					findpath(wrenchGaiaX, wrenchGaiaY - 1)
					sleep(3000)
				end
			end
			
			bot:setBubble(Bubble.brb)
			sleep(500)
			bot:wrench(wrenchGaiaX, wrenchGaiaY)
			addEvent(Event.variantlist, retrieveItems)
			listenEvents(5)
			bot:setBubble(Bubble.none)
		end
	end
	
	if bot.status ~= 1 then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		if inventory:getItemCount(Block_ID) > 0 then
			pnb()
		end
	end
	
	lastPNBBlockTileCheck()
	
	if Auto_Plant == "YES" then
		hiddenWorld()
		if not bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
			farmWorld()
		end
		
		webhookPlant()
		plant1()
	elseif Auto_Plant == "NO" then
		webhookAutoPlantNo()
		if inventory:getItemCount(Seed_ID) > 0 then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
			bot.auto_collect = false
			sleep(100)
			seedWorld()
			dropSeed()
			takeItem(Block_ID)
			hiddenWorld()
			farmWorld()
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
		end
		
		findpathBlock()
		harvest1()
	end
end

function pnbLast()
	if inventory:getItemCount(Block_ID) > 0 then
		if tutorialWorld == "nil" and Break_Other_World == "YES" and Use_Tutorial_World_For_Break == "YES" and not string.find(tutorialWorld, bot.name:upper()) then
			bot:sendPacket(2, "action|wrench\n|netid|" .. world:getLocal().netid)
			sleep(500)
			bot:sendPacket(2, "action|dialog_return\ndialog_name|popup\nnetID|" .. world:getLocal().netid .. "|\nbuttonClicked|my_worlds")
			addEvent(Event.variantlist, tutorialWorldEvent)
			listenEvents(2)
		end
		
		if Break_Other_World == "NO" and not bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
			farmWorld()
		elseif Break_Other_World == "YES" and (Use_Custom_World_For_Break == "YES" and not bot:isInWorld(Custom_World_Break_Other_World)) or (Use_Random_World_For_Break_Other_Break == "YES" and not bot:isInWorld(randomWorldBreak())) or (Use_Tutorial_World_For_Break == "YES" and not bot:isInWorld(tutorialWorld)) then
			breakOtherWorld()
		end
		
		randomSkinColor()
		bot:setDirection(true)
		sleep(100)
		
		if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
			bot.auto_collect = true
			sleep(100)
		end
		
		local checkValidPosition = true
		if checkValidPosition then
			if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and not bot:isInTile(X_Break, Y_Break) then
				reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
				findpath(X_Break, Y_Break)
			elseif Use_Custom_World_For_Break == "YES" and bot:isInWorld(Custom_World_Break_Other_World) and not bot:isInTile(X_Break, Y_Break) then
				reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
				findpath(X_Break, Y_Break)
			elseif Use_Random_World_For_Break_Other_Break == "YES" and bot:isInWorld(randomWorldBreak()) and not bot:isInTile(breakOtherWorldX, breakOtherWorldY) then
				reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
				findpath(breakOtherWorldX, breakOtherWorldY)
			elseif Use_Tutorial_World_For_Break == "YES" and bot:isInWorld(tutorialWorld) and not bot:isInTile(X_Break, Y_Break) then
				reconnect(tutorialWorld, X_Break, Y_Break)
				findpath(X_Break, Y_Break)
			end
		end
		
		while bot.status == 1 and inventory:getItemCount(Block_ID) > 0 and Break_Other_World == "NO" do
			if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and Auto_Retrieve == "NO" and bot.auto_collect ~= true then
				bot.auto_collect = true
				sleep(100)
			elseif bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and Auto_Retrieve == "YES" and bot.auto_collect ~= false then
				bot.auto_collect = false
				sleep(100)
			end
			
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
			
			if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and world:getTile(math.floor(world:getLocal().posx / 32), math.floor(world:getLocal().posy / 32)).fg == 0 and Auto_Plant == "YES" then
				bot:place(math.floor(world:getLocal().posx / 32), math.floor(world:getLocal().posy / 32), Seed_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			end
			
			webhookShareInfoAllBot()
			modEntered()
			autoConsume()
			randomChat()
			levelCheck()
			banWaveEstimated()
			playTime()
			
			if emptyTileScan() > remainingTile and inventory:getItemCount(Seed_ID) > 150 and Auto_Plant == "YES" then
				webhookPlant()
				plant1()
			elseif emptyTileScan() == remainingTile and inventory:getItemCount(Seed_ID) > 150 then
				bot.auto_collect = false
				sleep(100)
				seedWorld()
				dropSeed()
				takeItem(Block_ID)
				hiddenWorld()
				farmWorld()
				findpathBreak()
				bot:setDirection(true)
				sleep(100)
				if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
					bot.auto_collect = true
					sleep(100)
				end
			end
			
			if checkValidPosition then
				while bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and not bot:isInTile(X_Break, Y_Break) do
					sleep(25000)
					reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
					findpath(X_Break, Y_Break)
				end
			end
			
			if not bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
				farmWorld()
				findpathBreak()
			elseif bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
				breaker()
				
				if Break_Other_World == "YES" then
					if (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Custom_World_For_Break == "YES" and bot:isInWorld(Custom_World_Break_Other_World) then
						dropExceedPackGoOffline()
				
						while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
							seedWorld()
							dropSeed()
							while inventory:getItemCount(Block_ID) > 0 do
								dropBlock()
							end
							hiddenWorld()
						end
						
						while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
							pickaxeWorld()
							while not bot:isInTile(44, 13) do
								findpath(44, 13)
							end
							dropItem(Pickaxe_ID)
							if inventory:getItemCount(Pickaxe_ID) == 0 then
								webhookShareInfoDropped(
									"**[ BOT INFORMATION ]** \n" ..
									"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
									":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
									"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
									"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
									"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
									
									"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
									"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
									":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
									getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
								)
							end
							hiddenWorld()
						end
						
						dropEquipmentBeforeOfffline()
						
						autoRecon = false
						noStopScript = false
						bot:disconnect()
						sleep(100)
						webhookShareInfoAllBot()
						webhookShareInfoBot(
							"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
						)
						bot:stopScript()
						removeBot(bot.name)
						sleep(1000)
					elseif (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Random_World_For_Break_Other_Break == "YES" and bot:isInWorld(randomWorldBreak()) then
						cachedWorldBreak = nil
						pnb()
					elseif (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Tutorial_World_For_Break == "YES" and bot:isInWorld(tutorialWorld) then
						dropExceedPackGoOffline()
				
						while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
							seedWorld()
							dropSeed()
							while inventory:getItemCount(Block_ID) > 0 do
								dropBlock()
							end
							hiddenWorld()
						end
						
						while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
							pickaxeWorld()
							while not bot:isInTile(44, 13) do
								findpath(44, 13)
							end
							dropItem(Pickaxe_ID)
							if inventory:getItemCount(Pickaxe_ID) == 0 then
								webhookShareInfoDropped(
									"**[ BOT INFORMATION ]** \n" ..
									"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
									":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
									"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
									"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
									"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
									
									"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
									"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
									":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
									getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
								)
							end
							hiddenWorld()
						end
						
						dropEquipmentBeforeOfffline()
						
						autoRecon = false
						noStopScript = false
						bot:disconnect()
						sleep(100)
						webhookShareInfoAllBot()
						webhookShareInfoBot(
							"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
						)
						bot:stopScript()
						removeBot(bot.name)
						sleep(1000)
					end
				elseif Break_Other_World == "NO" then
					if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
						if world:hasAccess(bot.x + 1, bot.y) == 0 then
							dropExceedPackGoOffline()
				
							while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
								seedWorld()
								dropSeed()
								while inventory:getItemCount(Block_ID) > 0 do
									dropBlock()
								end
								hiddenWorld()
							end
							
							while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
								pickaxeWorld()
								while not bot:isInTile(44, 13) do
									findpath(44, 13)
								end
								dropItem(Pickaxe_ID)
								if inventory:getItemCount(Pickaxe_ID) == 0 then
									webhookShareInfoDropped(
										"**[ BOT INFORMATION ]** \n" ..
										"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
										":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
										"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
										"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
										"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
										
										"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
										"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
										":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
										getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
									)
								end
								hiddenWorld()
							end
							
							dropEquipmentBeforeOfffline()
						
							autoRecon = false
							noStopScript = false
							bot:disconnect()
							sleep(100)
							webhookShareInfoAllBot()
							webhookShareInfoBot(
								"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
							)
							bot:stopScript()
							removeBot(bot.name)
							sleep(1000)
						elseif tileGrowScan(14142) > 0 then
							antiFire()
						end
					end
				end
			end
		end
		
		while bot.status == 1 and inventory:getItemCount(Block_ID) > 0 and Break_Other_World == "YES" do
			if (bot:isInWorld(Custom_World_Break_Other_World) or bot:isInWorld(randomWorldBreak()) or bot:isInWorld(tutorialWorld)) and Auto_Retrieve == "NO" and bot.auto_collect ~= true then
				bot.auto_collect = true
				sleep(100)
			elseif (bot:isInWorld(Custom_World_Break_Other_World) or bot:isInWorld(randomWorldBreak()) or bot:isInWorld(tutorialWorld)) and Auto_Retrieve == "YES" and bot.auto_collect ~= false then
				bot.auto_collect = false
				sleep(100)
			end
			
			if Break_Other_World == "YES" and Use_Custom_World_For_Break == "YES" then
				reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
			elseif Break_Other_World == "YES" and Use_Random_World_For_Break_Other_Break == "YES" then
				reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
			end
			
			webhookShareInfoAllBot()
			modEntered()
			autoConsume()
			randomChat()
			levelCheck()
			banWaveEstimated()
			playTime()
			
			if emptyTileScan() > remainingTile and inventory:getItemCount(Seed_ID) > 150 and Auto_Plant == "YES" then
				farmWorld()
				webhookPlant()
				plant1()
			elseif emptyTileScan() == remainingTile and inventory:getItemCount(Seed_ID) > 150 then
				bot.auto_collect = false
				sleep(100)
				seedWorld()
				dropSeed()
				takeItem(Block_ID)
				hiddenWorld()
				breakOtherWorld()
				findpathBreakOtherWorld()
				bot:setDirection(true)
				sleep(100)
				if bot:isInWorld(randomWorldBreak()) then
					bot.auto_collect = true
					sleep(100)
				end
			end
			
			if checkValidPosition then
				while Use_Custom_World_For_Break == "YES" and bot:isInWorld(Custom_World_Break_Other_World) and not bot:isInTile(X_Break, Y_Break) do
					sleep(25000)
					reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
					findpath(X_Break, Y_Break)
				end
				
				while Use_Random_World_For_Break_Other_Break == "YES" and bot:isInWorld(randomWorldBreak()) and not bot:isInTile(breakOtherWorldX, breakOtherWorldY) do
					sleep(25000)
					reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
					findpath(breakOtherWorldX, breakOtherWorldY)
				end
				
				while Use_Tutorial_World_For_Break == "YES" and bot:isInWorld(tutorialWorld) and not bot:isInTile(X_Break, Y_Break) do
					sleep(25000)
					reconnect(tutorialWorld, X_Break, Y_Break)
					findpath(X_Break, Y_Break)
				end
			end
			
			if (Use_Custom_World_For_Break == "YES" and not bot:isInWorld(Custom_World_Break_Other_World)) or (Use_Random_World_For_Break_Other_Break == "YES" and not bot:isInWorld(randomWorldBreak())) or (Use_Tutorial_World_For_Break == "YES" and not bot:isInWorld(tutorialWorld)) then
				breakOtherWorld()
				findpathBreakOtherWorld()
			elseif (bot:isInWorld(Custom_World_Break_Other_World) or bot:isInWorld(randomWorldBreak()) or bot:isInWorld(tutorialWorld)) then
				breaker()
				
				if Break_Other_World == "YES" then
					if (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Custom_World_For_Break == "YES" and bot:isInWorld(Custom_World_Break_Other_World) then
						dropExceedPackGoOffline()
				
						while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
							seedWorld()
							dropSeed()
							while inventory:getItemCount(Block_ID) > 0 do
								dropBlock()
							end
							hiddenWorld()
						end
						
						while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
							pickaxeWorld()
							while not bot:isInTile(44, 13) do
								findpath(44, 13)
							end
							dropItem(Pickaxe_ID)
							if inventory:getItemCount(Pickaxe_ID) == 0 then
								webhookShareInfoDropped(
									"**[ BOT INFORMATION ]** \n" ..
									"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
									":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
									"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
									"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
									"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
									
									"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
									"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
									":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
									getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
								)
							end
							hiddenWorld()
						end
						
						dropEquipmentBeforeOfffline()
						
						autoRecon = false
						noStopScript = false
						bot:disconnect()
						sleep(100)
						webhookShareInfoAllBot()
						webhookShareInfoBot(
							"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
						)
						bot:stopScript()
						removeBot(bot.name)
						sleep(1000)
					elseif (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Random_World_For_Break_Other_Break == "YES" and bot:isInWorld(randomWorldBreak()) then
						cachedWorldBreak = nil
						pnb()
					elseif (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Tutorial_World_For_Break == "YES" and bot:isInWorld(tutorialWorld) then
						dropExceedPackGoOffline()
				
						while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
							seedWorld()
							dropSeed()
							while inventory:getItemCount(Block_ID) > 0 do
								dropBlock()
							end
							hiddenWorld()
						end
						
						while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
							pickaxeWorld()
							while not bot:isInTile(44, 13) do
								findpath(44, 13)
							end
							dropItem(Pickaxe_ID)
							if inventory:getItemCount(Pickaxe_ID) == 0 then
								webhookShareInfoDropped(
									"**[ BOT INFORMATION ]** \n" ..
									"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
									":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
									"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
									"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
									"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
									
									"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
									"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
									":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
									getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
								)
							end
							hiddenWorld()
						end
						
						dropEquipmentBeforeOfffline()
						
						autoRecon = false
						noStopScript = false
						bot:disconnect()
						sleep(100)
						webhookShareInfoAllBot()
						webhookShareInfoBot(
							"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
						)
						bot:stopScript()
						removeBot(bot.name)
						sleep(1000)
					end
				elseif Break_Other_World == "NO" then
					if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
						if world:hasAccess(bot.x + 1, bot.y) == 0 then
							dropExceedPackGoOffline()
				
							while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
								seedWorld()
								dropSeed()
								while inventory:getItemCount(Block_ID) > 0 do
									dropBlock()
								end
								hiddenWorld()
							end
							
							while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
								pickaxeWorld()
								while not bot:isInTile(44, 13) do
									findpath(44, 13)
								end
								dropItem(Pickaxe_ID)
								if inventory:getItemCount(Pickaxe_ID) == 0 then
									webhookShareInfoDropped(
										"**[ BOT INFORMATION ]** \n" ..
										"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
										":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
										"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
										"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
										"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
										
										"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
										"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
										":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
										getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
									)
								end
								hiddenWorld()
							end
							
							dropEquipmentBeforeOfffline()
						
							autoRecon = false
							noStopScript = false
							bot:disconnect()
							sleep(100)
							webhookShareInfoAllBot()
							webhookShareInfoBot(
								"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
							)
							bot:stopScript()
							removeBot(bot.name)
							sleep(1000)
						elseif tileGrowScan(14142) > 0 then
							antiFire()
						end
					end
				end
			end
		end
	end
	
	webhookShareInfoAllBot()
	
	if Auto_Retrieve == "YES" and bot.status == 1 then
		local retrieveUnstable = true
		if retrieveUnstable then
			if wrenchUnstableX == nil and wrenchUnstableY == nil then
				for _, tile in pairs(world:getTiles()) do
					if bot.status == 1 then
						if tile.fg == 6948 then
							findpath(tile.x, tile.y - 1)
							while not bot:isInTile(tile.x, tile.y - 1) do
								findpath(tile.x, tile.y - 1)
								sleep(3000)
							end
							
							wrenchUnstableX = tile.x
							wrenchUnstableY = tile.y
							break
						end
					elseif bot.status ~= 1 or (bot.status == 1 and bot:getPing() == 0) then
						defaultBotX = bot.x
						defaultBotY = bot.y
						
						if Break_Other_World == "YES" then
							if Use_Custom_World_For_Break == "YES" then
								reconnect(Custom_World_Break_Other_World, defaultBotX, defaultBotY)
							elseif Use_Random_World_For_Break_Other_Break == "YES" then
								reconnect(randomWorldBreak(), defaultBotX, defaultBotY)
							elseif Use_Tutorial_World_For_Break == "YES" then
								reconnect(tutorialWorld, defaultBotX, defaultBotY)
							end
						elseif Break_Other_World == "NO" then
							reconnect(Rotation_World[rotationWorldStartNumber]:upper(), defaultBotX, defaultBotY)
						end
					end
				end
			else
				findpath(wrenchUnstableX, wrenchUnstableY - 1)
				while not bot:isInTile(wrenchUnstableX, wrenchUnstableY - 1) do
					findpath(wrenchUnstableX, wrenchUnstableY - 1)
					sleep(3000)
				end
			end
			
			bot:setBubble(Bubble.brb)
			sleep(500)
			bot:wrench(wrenchUnstableX, wrenchUnstableY)
			addEvent(Event.variantlist, retrieveItems)
			listenEvents(5)
			bot:setBubble(Bubble.none)
		end
		
		if inventory:getItemCount(Block_ID) > 0 then
			pnbLast()
		end
		
		local retrieveGaia = true
		if retrieveGaia then
			if wrenchGaiaX == nil and wrenchGaiaY == nil then
				for _, tile in pairs(world:getTiles()) do
					if bot.status == 1 then
						if tile.fg == 6946 then
							findpath(tile.x, tile.y - 1)
							while not bot:isInTile(tile.x, tile.y - 1) do
								findpath(tile.x, tile.y - 1)
								sleep(3000)
							end
							
							wrenchGaiaX = tile.x
							wrenchGaiaY = tile.y
							break
						end
					elseif bot.status ~= 1 or (bot.status == 1 and bot:getPing() == 0) then
						defaultBotX = bot.x
						defaultBotY = bot.y
						
						if Break_Other_World == "YES" then
							if Use_Custom_World_For_Break == "YES" then
								reconnect(Custom_World_Break_Other_World, defaultBotX, defaultBotY)
							elseif Use_Random_World_For_Break_Other_Break == "YES" then
								reconnect(randomWorldBreak(), defaultBotX, defaultBotY)
							elseif Use_Tutorial_World_For_Break == "YES" then
								reconnect(tutorialWorld, defaultBotX, defaultBotY)
							end
						elseif Break_Other_World == "NO" then
							reconnect(Rotation_World[rotationWorldStartNumber]:upper(), defaultBotX, defaultBotY)
						end
					end
				end
			else
				findpath(wrenchGaiaX, wrenchGaiaY - 1)
				while not bot:isInTile(wrenchGaiaX, wrenchGaiaY - 1) do
					findpath(wrenchGaiaX, wrenchGaiaY - 1)
					sleep(3000)
				end
			end
				
			bot:setBubble(Bubble.brb)
			sleep(500)
			bot:wrench(wrenchGaiaX, wrenchGaiaY)
			addEvent(Event.variantlist, retrieveItems)
			listenEvents(5)
			bot:setBubble(Bubble.none)
		end
	end
	
	if bot.status ~= 1 then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		if inventory:getItemCount(Block_ID) > 0 then
			pnbLast()
		end
	end
	
	lastPNBBlockTileCheck()
	
	if Auto_Plant == "YES" then
		hiddenWorld()
		if not bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
			farmWorld()
		end
	elseif Auto_Plant == "NO" then
		webhookAutoPlantNo()
		if inventory:getItemCount(Seed_ID) > 0 then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
			bot.auto_collect = false
			sleep(100)
			seedWorld()
			dropSeed()
			takeItem(Block_ID)
			hiddenWorld()
			farmWorld()
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
		end
	end
	
	lastCheck1()
end

function lastPNBBlockTileCheck()
	if Break_Other_World == "YES" then
		if Use_Custom_World_For_Break == "YES" then
			reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
		elseif Use_Random_World_For_Break_Other_Break == "YES" then
			reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
		elseif Use_Tutorial_World_For_Break == "YES" then
			reconnect(tutorialWorld, X_Break, Y_Break)
		end
	elseif Break_Other_World == "NO" then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
	end
	
	if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
		for _, tile in pairs(world:getTiles()) do
			if bot.status == 1 then
				if (tile.fg == Block_ID or tile.bg == Block_ID) then
					if tile.y == 0 and world:hasAccess(tile.x, tile.y) ~= 0 then
						if Break_Other_World == "YES" then
							if Use_Custom_World_For_Break == "YES" then
								reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
							elseif Use_Random_World_For_Break_Other_Break == "YES" then
								reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
							elseif Use_Tutorial_World_For_Break == "YES" then
								reconnect(tutorialWorld, X_Break, Y_Break)
							end
						elseif Break_Other_World == "NO" then
							reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
						end
						
						if #bot:getPath(tile.x, tile.y + 1) > 0 then
							findpath(tile.x, tile.y + 1)
							
							while not bot:isInTile(tile.x, tile.y + 1) and #bot:getPath(tile.x, tile.y + 1) > 0 do
								sleep(25000)
								if Break_Other_World == "YES" then
									if Use_Custom_World_For_Break == "YES" then
										reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
									elseif Use_Random_World_For_Break_Other_Break == "YES" then
										reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
									elseif Use_Tutorial_World_For_Break == "YES" then
										reconnect(tutorialWorld, X_Break, Y_Break)
									end
								elseif Break_Other_World == "NO" then
									reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
								end
								
								findpath(tile.x, tile.y + 1)
							end
							
							destroyDefault(tile.x, tile.y)
						elseif #bot:getPath(tile.x, tile.y + 1) == 0 and bot:isInTile(tile.x, tile.y + 1) then
							destroyDefault(tile.x, tile.y)
						end
					elseif tile.y == 1 and world:hasAccess(tile.x, tile.y) ~= 0 then
						if Break_Other_World == "YES" then
							if Use_Custom_World_For_Break == "YES" then
								reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
							elseif Use_Random_World_For_Break_Other_Break == "YES" then
								reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
							elseif Use_Tutorial_World_For_Break == "YES" then
								reconnect(tutorialWorld, X_Break, Y_Break)
							end
						elseif Break_Other_World == "NO" then
							reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
						end
						
						if #bot:getPath(tile.x, tile.y - 1) > 0 then
							findpath(tile.x, tile.y - 1)
							
							while not bot:isInTile(tile.x, tile.y - 1) and #bot:getPath(tile.x, tile.y - 1) > 0 do
								sleep(25000)
								if Break_Other_World == "YES" then
									if Use_Custom_World_For_Break == "YES" then
										reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
									elseif Use_Random_World_For_Break_Other_Break == "YES" then
										reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
									elseif Use_Tutorial_World_For_Break == "YES" then
										reconnect(tutorialWorld, X_Break, Y_Break)
									end
								elseif Break_Other_World == "NO" then
									reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
								end
								
								findpath(tile.x, tile.y - 1)
							end
							
							destroyDefault(tile.x, tile.y)
						elseif #bot:getPath(tile.x, tile.y - 1) == 0 and bot:isInTile(tile.x, tile.y - 1)	 then
							destroyDefault(tile.x, tile.y)
						end
					else
						if world:hasAccess(tile.x, tile.y) ~= 0 then
							if Break_Other_World == "YES" then
								if Use_Custom_World_For_Break == "YES" then
									reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
								elseif Use_Random_World_For_Break_Other_Break == "YES" then
									reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
								elseif Use_Tutorial_World_For_Break == "YES" then
									reconnect(tutorialWorld, X_Break, Y_Break)
								end
							elseif Break_Other_World == "NO" then
								reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
							end
						
							if #bot:getPath(tile.x, tile.y - 2) > 0 then
								findpath(tile.x, tile.y - 2)
								
								while not bot:isInTile(tile.x, tile.y - 2) and #bot:getPath(tile.x, tile.y - 2) > 0 do
									sleep(25000)
									if Break_Other_World == "YES" then
										if Use_Custom_World_For_Break == "YES" then
											reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
										elseif Use_Random_World_For_Break_Other_Break == "YES" then
											reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
										elseif Use_Tutorial_World_For_Break == "YES" then
											reconnect(tutorialWorld, X_Break, Y_Break)
										end
									elseif Break_Other_World == "NO" then
										reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
									end
									
									findpath(tile.x, tile.y - 2)
								end
								
								destroyDefault(tile.x, tile.y)
							elseif #bot:getPath(tile.x, tile.y - 2) == 0 and bot:isInTile(tile.x, tile.y - 2) then
								destroyDefault(tile.x, tile.y)
							end
						end
					end
				end
			elseif bot.status ~= 1 or (bot.status == 1 and bot:getPing() == 0) then
				if Break_Other_World == "YES" then
					if Use_Custom_World_For_Break == "YES" then
						reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
					elseif Use_Random_World_For_Break_Other_Break == "YES" then
						reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
					elseif Use_Tutorial_World_For_Break == "YES" then
						reconnect(tutorialWorld, X_Break, Y_Break)
					end
				elseif Break_Other_World == "NO" then
					reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
				end
			end
		end
	end
end

function destroy(x, y)
	while bot.status == 1 and (world:getTile(x, y).fg ~= 0 or world:getTile(x, y).bg ~= 0) and (bot:isInTile(X_Break, Y_Break) or bot:isInTile(breakOtherWorldX, breakOtherWorldY)) do
		if Break_Other_World == "YES" then
			if Use_Custom_World_For_Break == "YES" then
				reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
			elseif Use_Random_World_For_Break_Other_Break == "YES" then
				reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
			elseif Use_Tutorial_World_For_Break == "YES" then
				reconnect(tutorialWorld, X_Break, Y_Break)
			end
		elseif Break_Other_World == "NO" then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		end
		
		if not bot:isInTile(X_Break, Y_Break) and (bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) or bot:isInWorld(Custom_World_Break_Other_World)) then
			sleep(25000)
			if Break_Other_World == "YES" then
				if Use_Custom_World_For_Break == "YES" then
					reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
				elseif Use_Random_World_For_Break_Other_Break == "YES" then
					reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
				elseif Use_Tutorial_World_For_Break == "YES" then
					reconnect(tutorialWorld, X_Break, Y_Break)
				end
			elseif Break_Other_World == "NO" then
				reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
			end
			findpath(X_Break, Y_Break)
		elseif not bot:isInTile(breakOtherWorldX, breakOtherWorldY) and bot:isInWorld(randomWorldBreak()) then
			sleep(25000)
			if Break_Other_World == "YES" then
				if Use_Custom_World_For_Break == "YES" then
					reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
				elseif Use_Random_World_For_Break_Other_Break == "YES" then
					reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
				elseif Use_Tutorial_World_For_Break == "YES" then
					reconnect(tutorialWorld, X_Break, Y_Break)
				end
			elseif Break_Other_World == "NO" then
				reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
			end
			findpath(breakOtherWorldX, breakOtherWorldY)
		end
		
		if Break_Other_World == "YES" then
			if (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Custom_World_For_Break == "YES" and bot:isInWorld(Custom_World_Break_Other_World) then
				dropExceedPackGoOffline()
		
				while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
					seedWorld()
					dropSeed()
					while inventory:getItemCount(Block_ID) > 0 do
						dropBlock()
					end
					hiddenWorld()
				end
				
				while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
					pickaxeWorld()
					while not bot:isInTile(44, 13) do
						findpath(44, 13)
					end
					dropItem(Pickaxe_ID)
					if inventory:getItemCount(Pickaxe_ID) == 0 then
						webhookShareInfoDropped(
							"**[ BOT INFORMATION ]** \n" ..
							"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
							":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
							"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
							"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
							"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
							
							"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
							"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
							":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
							getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
						)
					end
					hiddenWorld()
				end
				
				dropEquipmentBeforeOfffline()
				
				autoRecon = false
				noStopScript = false
				bot:disconnect()
				sleep(100)
				webhookShareInfoAllBot()
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			elseif (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Random_World_For_Break_Other_Break == "YES" and bot:isInWorld(randomWorldBreak()) then
				cachedWorldBreak = nil
				pnb()
			elseif (world:hasAccess(bot.x + 1, bot.y) == 0 or tileGrowScan(14142) > 0) and Use_Tutorial_World_For_Break == "YES" and bot:isInWorld(tutorialWorld) then
				dropExceedPackGoOffline()
		
				while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
					seedWorld()
					dropSeed()
					while inventory:getItemCount(Block_ID) > 0 do
						dropBlock()
					end
					hiddenWorld()
				end
				
				while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
					pickaxeWorld()
					while not bot:isInTile(44, 13) do
						findpath(44, 13)
					end
					dropItem(Pickaxe_ID)
					if inventory:getItemCount(Pickaxe_ID) == 0 then
						webhookShareInfoDropped(
							"**[ BOT INFORMATION ]** \n" ..
							"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
							":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
							"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
							"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
							"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
							
							"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
							"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
							":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
							getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
						)
					end
					hiddenWorld()
				end
				
				dropEquipmentBeforeOfffline()
				
				autoRecon = false
				noStopScript = false
				bot:disconnect()
				sleep(100)
				webhookShareInfoAllBot()
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			end
		elseif Break_Other_World == "NO" then
			if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
				if world:hasAccess(bot.x + 1, bot.y) == 0 then
					dropExceedPackGoOffline()
		
					while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
						seedWorld()
						dropSeed()
						while inventory:getItemCount(Block_ID) > 0 do
							dropBlock()
						end
						hiddenWorld()
					end
					
					while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
						pickaxeWorld()
						while not bot:isInTile(44, 13) do
							findpath(44, 13)
						end
						dropItem(Pickaxe_ID)
						if inventory:getItemCount(Pickaxe_ID) == 0 then
							webhookShareInfoDropped(
								"**[ BOT INFORMATION ]** \n" ..
								"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
								":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
								"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
								"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
								"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
								
								"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
								"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
								":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
								getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
							)
						end
						hiddenWorld()
					end
					
					dropEquipmentBeforeOfffline()
				
					autoRecon = false
					noStopScript = false
					bot:disconnect()
					sleep(100)
					webhookShareInfoAllBot()
					webhookShareInfoBot(
						"<a:toa:997296837352505415> **|** " .. BotName .. " is offline as it does not have access to PNB."
					)
					bot:stopScript()
					removeBot(bot.name)
					sleep(1000)
				elseif tileGrowScan(14142) > 0 then
					antiFire()
				end
			end
		end
		
		if bot:getPing() < High_Ping and world:hasAccess(x, y) ~= 0 then
			bot:hit(x, y)
			sleep(math.random(Hit_Delay, Hit_Delay + 10))
		elseif bot:getPing() >= High_Ping and world:hasAccess(x, y) ~= 0 then
			bot:hit(x, y)
			sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
		end
		
		if Break_Other_World == "YES" then
			if Use_Custom_World_For_Break == "YES" then
				reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
			elseif Use_Random_World_For_Break_Other_Break == "YES" then
				reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
			elseif Use_Tutorial_World_For_Break == "YES" then
				reconnect(tutorialWorld, X_Break, Y_Break)
			end
		elseif Break_Other_World == "NO" then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		end
	end
	
	if Break_Other_World == "YES" then
		if Use_Custom_World_For_Break == "YES" then
			reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
		elseif Use_Random_World_For_Break_Other_Break == "YES" then
			reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
		elseif Use_Tutorial_World_For_Break == "YES" then
			reconnect(tutorialWorld, X_Break, Y_Break)
		end
	elseif Break_Other_World == "NO" then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
	end
end

function destroyDefault(x, y)
	while bot.status == 1 and (world:getTile(x, y).fg ~= 0 or world:getTile(x, y).bg ~= 0) do
		if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		else
			defaultWorldName = world.name:upper()
			defaultBotX = bot.x
			defaultBotY = bot.y
				
			reconnect(defaultWorldName, defaultBotX, defaultBotY)
		end
		
		if bot:getPing() < High_Ping and world:hasAccess(x, y) ~= 0 then
			bot:hit(x, y)
			sleep(math.random(Hit_Delay, Hit_Delay + 10))
		elseif bot:getPing() >= High_Ping and world:hasAccess(x, y) ~= 0 then
			bot:hit(x, y)
			sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
		end
		
		if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		else
			defaultWorldName = world.name:upper()
			defaultBotX = bot.x
			defaultBotY = bot.y
				
			reconnect(defaultWorldName, defaultBotX, defaultBotY)
		end
	end
	
	if bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
	else
		defaultWorldName = world.name:upper()
		defaultBotY = bot.y
		defaultBotX = bot.x
				
		reconnect(defaultWorldName, defaultBotX, defaultBotY)
	end
end

function breaker()
	if Break_Other_World == "YES" and Use_Tutorial_World_For_Break == "YES" and bot:isInWorld(tutorialWorld) and playerScan() > 0 then
		for _, player in pairs(world:getPlayers()) do
			if not string.find(player.name:upper(), bot.name:upper()) then
				bot:setBubble(Bubble.talk)
				sleep(500)
				bot:say("/ban " .. player.name)
				bot:setBubble(Bubble.none)
			end
		end
	end
	
	if math.floor(world:getLocal().posx / 32) == X_Break and math.floor(world:getLocal().posy / 32) == Y_Break then
		if Break_Other_World == "YES" then
			if Use_Custom_World_For_Break == "YES" then
				reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
			elseif Use_Random_World_For_Break_Other_Break == "YES" then
				reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
			elseif Use_Tutorial_World_For_Break == "YES" then	
				reconnect(tutorialWorld, X_Break, Y_Break)
			end
		elseif Break_Other_World == "NO" then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		end
		
		if Custom_Break_Tile == "5" then
			if bot:getPing() < High_Ping then
				bot:place(X_Break - 1, Y_Break - 2, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break + 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break + 2, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(X_Break - 1, Y_Break - 2, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break + 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break + 2, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(X_Break - 1, Y_Break - 2)
			destroy(X_Break - 1, Y_Break - 1)
			destroy(X_Break - 1, Y_Break)
			destroy(X_Break - 1, Y_Break + 1)
			destroy(X_Break - 1, Y_Break + 2)
		elseif Custom_Break_Tile == "4" then
			if bot:getPing() < High_Ping then
				bot:place(X_Break - 1, Y_Break - 2, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break + 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(X_Break - 1, Y_Break - 2, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break + 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(X_Break - 1, Y_Break - 2)
			destroy(X_Break - 1, Y_Break - 1)
			destroy(X_Break - 1, Y_Break)
			destroy(X_Break - 1, Y_Break + 1)
		elseif Custom_Break_Tile == "3" then
			if bot:getPing() < High_Ping then
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break + 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break + 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(X_Break - 1, Y_Break - 1)
			destroy(X_Break - 1, Y_Break)
			destroy(X_Break - 1, Y_Break + 1)
		elseif Custom_Break_Tile == "2" then
			if bot:getPing() < High_Ping then
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(X_Break - 1, Y_Break - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(X_Break - 1, Y_Break - 1)
			destroy(X_Break - 1, Y_Break)
		elseif Custom_Break_Tile == "1" then
			if bot:getPing() < High_Ping then
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(X_Break - 1, Y_Break, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(X_Break - 1, Y_Break)
		else 
			webhookShareInfoBot(
				"<a:toa:997296837352505415> **|** Enter Custom_Break_Tile with the numbers 1 to 5, according to how many tile you want to break."
			)
		end
	elseif math.floor(world:getLocal().posx / 32) == breakOtherWorldX and math.floor(world:getLocal().posy / 32) == breakOtherWorldY then
		if Break_Other_World == "YES" then
			if Use_Custom_World_For_Break == "YES" then
				reconnect(Custom_World_Break_Other_World, X_Break, Y_Break)
			elseif Use_Random_World_For_Break_Other_Break == "YES" then
				reconnect(randomWorldBreak(), breakOtherWorldX, breakOtherWorldY)
			elseif Use_Tutorial_World_For_Break == "YES" then
				reconnect(tutorialWorld, X_Break, Y_Break)
			end
		elseif Break_Other_World == "NO" then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), X_Break, Y_Break)
		end
		
		if Custom_Break_Tile == "5" then
			if bot:getPing() < High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 2, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 2, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 2, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 2, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(breakOtherWorldX - 1, breakOtherWorldY - 2)
			destroy(breakOtherWorldX - 1, breakOtherWorldY - 1)
			destroy(breakOtherWorldX - 1, breakOtherWorldY)
			destroy(breakOtherWorldX - 1, breakOtherWorldY + 1)
			destroy(breakOtherWorldX - 1, breakOtherWorldY + 2)
		elseif Custom_Break_Tile == "4" then
			if bot:getPing() < High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 2, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 2, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(breakOtherWorldX - 1, breakOtherWorldY - 2)
			destroy(breakOtherWorldX - 1, breakOtherWorldY - 1)
			destroy(breakOtherWorldX - 1, breakOtherWorldY)
			destroy(breakOtherWorldX - 1, breakOtherWorldY + 1)
		elseif Custom_Break_Tile == "3" then
			if bot:getPing() < High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY + 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(breakOtherWorldX - 1, breakOtherWorldY - 1)
			destroy(breakOtherWorldX - 1, breakOtherWorldY)
			destroy(breakOtherWorldX - 1, breakOtherWorldY + 1)
		elseif Custom_Break_Tile == "2" then
			if bot:getPing() < High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY - 1, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(breakOtherWorldX - 1, breakOtherWorldY - 1)
			destroy(breakOtherWorldX - 1, breakOtherWorldY)
		elseif Custom_Break_Tile == "1" then
			if bot:getPing() < High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(Place_Delay, Place_Delay + 10))
			elseif bot:getPing() >= High_Ping then
				bot:place(breakOtherWorldX - 1, breakOtherWorldY, Block_ID)
				sleep(math.random(bot:getPing() + 50, bot:getPing() + 100))
			end
			destroy(breakOtherWorldX - 1, breakOtherWorldY)
		else 
			webhookShareInfoBot(
				"<a:toa:997296837352505415> **|** Enter Custom_Break_Tile with the numbers 1 to 5, according to how many tile you want to break."
			)
		end
	end
end
---- [ END PNB ] ----


---- [ LOOPING WORLD ] ----
function loopStatus()
	if Looping_World == "YES" then
		nextWorldLoop()
	elseif Looping_World == "NO" then
		nextWorldNoLoop()
	end
end

function nextWorldLoop()
	if nextWorldStatus == true and noStopScript == true then
		webhookShareInfoBot(
			"**[ BOT INFORMATION ]** \n" ..
			"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
			":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
			"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
			"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
			"<a:toa:997296837352505415> **|** Bot Information: Next World \n\n" ..
			
			"**[ ESTIMATED TIME ]** \n" ..
			"<:Globe:1190695520210006036> **|** World Status: `" .. Rotation_World[rotationWorldStartNumber]:upper() .. "` Finished \n" ..
			"<:Right_Arrow_V2:1190695167078957056>  `[Farm World " .. rotationWorldStartNumber .. " of " .. TotalRotationWorld .. " World]` \n" ..
			"<:Luxurious_Wall_Clock:1190695156333150258> **|** Estimated End Time: \n" ..
			"**" .. sonsaaat .. " Hours, " .. sondakika .. " Minutes/" .. job .. " Tree**"
		)
	elseif nextWorldStatus == false and noStopScript == true then
		table.remove(Rotation_World, rotationWorldStartNumber)
		TotalRotationWorld = #Rotation_World
		rotationWorldStartNumber = rotationWorldStartNumber - 1
		if TotalRotationWorld == 0 and noStopScript == true then
			autoRecon = false
			noStopScript = false
			bot:disconnect()
			sleep(100)
			webhookShareInfoAllBot()
			webhookShareInfoBot( 
				"<a:toa:997296837352505415> **|** Your world total in the rotation world list is " .. TotalRotationWorld .. ", so the script at this index will stop running."
			)
			bot:stopScript()
			removeBot(bot.name)
			sleep(1000)
		end
		nextWorldStatus = true
	elseif nextWorldStatus == "otherCondition" and noStopScript == true then
		table.remove(Rotation_World, rotationWorldStartNumber)
		TotalRotationWorld = #Rotation_World
		rotationWorldStartNumber = rotationWorldStartNumber - 1
		if TotalRotationWorld == 0 and noStopScript == true then
			autoRecon = false
			noStopScript = false
			bot:disconnect()
			sleep(100)
			webhookShareInfoAllBot()
			webhookShareInfoBot( 
				"<a:toa:997296837352505415> **|** Your world total in the rotation world list is " .. TotalRotationWorld .. ", so the script at this index will stop running."
			)
			bot:stopScript()
			removeBot(bot.name)
			sleep(1000)
		end
		nextWorldStatus = true
	end
	
	if nextWorldStatus == true and noStopScript == true then
		rotationWorldStartNumber = rotationWorldStartNumber + 1
		if rotationWorldStartNumber <= TotalRotationWorld then
			webhookShareInfoBot(
				"**[ BOT INFORMATION ]** \n" ..
				"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
				":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
				"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
				"<:Level:1190699579230916650> **|** Bot Level: " .. bot.level .. " \n" .. 
				"<:Purple_Gem:1190695162549108876> **|** Gems Amount: " .. bot.gem_count .. " \n\n" ..
				
				"**[ WORLD INFORMATION ]** \n" ..
				"<:Globe:1190695520210006036> **|** Current Farm World: `" .. Rotation_World[rotationWorldStartNumber]:upper() .. "` \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[Farm World " .. rotationWorldStartNumber .. " of " .. TotalRotationWorld .. " World]` \n" ..
				"<:Globe:1190695520210006036> **|** PNB World: `" .. breakOtherWorldStatus() .. "` \n" ..
				"<:Globe:1190695520210006036> **|** Seed World: `" .. Save_Seed_World .. "` \n" ..
				"<:Globe:1190695520210006036> **|** Pack World: `" .. Save_Pack_World .. "` \n\n" ..
				
				"**[ TREE INFORMATION ]** \n" ..
				"<:Laser_Grid_Tree:1190695145188884661> **|** Farmed Tree: " .. getItemName(Block_ID) .. " \n\n" ..
				
				"**[ DROPPED ITEM INFORMATION ]** \n" ..
				"<:Seed_And_Block:1190695174993608805> **|** Total Seed and Block Dropped: \n" ..
				getItemName(Seed_ID) .. ": " .. dropSeedCount .. " \n" ..
				getItemName(Block_ID) .. ": " .. dropBlockCount .. " \n\n" ..
				":package: **|** Total Pack Dropped:" ..
				dropPackCount .. " \n\n" ..
				
				"**[ TIME INFORMATION ]** \n" ..
				"<:Luxurious_Wall_Clock:1190695156333150258> **|** Online Time: " .. secondON(ge)
			)
			main()
		else
			rotationWorldStartNumber = 1
			webhookShareInfoBot(
				"**[ BOT INFORMATION ]** \n" ..
				"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
				":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
				"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
				"<:Level:1190699579230916650> **|** Bot Level: " .. bot.level .. " \n" .. 
				"<:Purple_Gem:1190695162549108876> **|** Gems Amount: " .. bot.gem_count .. " \n\n" ..
				
				"**[ WORLD INFORMATION ]** \n" ..
				"<:Globe:1190695520210006036> **|** Current Farm World: `" .. Rotation_World[rotationWorldStartNumber]:upper() .. "` \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[Farm World " .. rotationWorldStartNumber .. " of " .. TotalRotationWorld .. " World]` \n" ..
				"<:Globe:1190695520210006036> **|** PNB World: `" .. breakOtherWorldStatus() .. "` \n" ..
				"<:Globe:1190695520210006036> **|** Seed World: `" .. Save_Seed_World .. "` \n" ..
				"<:Globe:1190695520210006036> **|** Pack World: `" .. Save_Pack_World .. "` \n\n" ..
				
				"**[ TREE INFORMATION ]** \n" ..
				"<:Laser_Grid_Tree:1190695145188884661> **|** Farmed Tree: " .. getItemName(Block_ID) .. " \n\n" ..
				
				"**[ DROPPED ITEM INFORMATION ]** \n" ..
				"<:Seed_And_Block:1190695174993608805> **|** Total Seed and Block Dropped: \n" ..
				getItemName(Seed_ID) .. ": " .. dropSeedCount .. " \n" ..
				getItemName(Block_ID) .. ": " .. dropBlockCount .. " \n\n" ..
				":package: **|** Total Pack Dropped:" ..
				dropPackCount .. " \n\n" ..
				
				"**[ TIME INFORMATION ]** \n" ..
				"<:Luxurious_Wall_Clock:1190695156333150258> **|** Online Time: " .. secondON(ge)
			)
			main()
		end
	end
end

function nextWorldNoLoop()
	if nextWorldStatus == true and noStopScript == true then
		webhookShareInfoBot(
			"**[ BOT INFORMATION ]** \n" ..
			"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
			":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
			"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
			"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
			"<a:toa:997296837352505415> **|** Bot Information: Next World \n\n" ..
			
			"**[ ESTIMATED TIME ]** \n" ..
			"<:Globe:1190695520210006036> **|** World Status: `" .. Rotation_World[rotationWorldStartNumber]:upper() .. "` Finished \n" ..
			"<:Right_Arrow_V2:1190695167078957056>  `[Farm World " .. rotationWorldStartNumber .. " of " .. TotalRotationWorld .. " World]` \n" ..
			"<:Luxurious_Wall_Clock:1190695156333150258> **|** Estimated End Time: \n" ..
			"**" .. sonsaaat .. " Hours, " .. sondakika .. " Minutes/" .. job .. " Tree**"
		)
	elseif nextWorldStatus == false and noStopScript == true then
		table.remove(Rotation_World, rotationWorldStartNumber)
		TotalRotationWorld = #Rotation_World
		rotationWorldStartNumber = rotationWorldStartNumber - 1
		if TotalRotationWorld == 0 and noStopScript == true then
			autoRecon = false
			noStopScript = false
			bot:disconnect()
			sleep(100)
			webhookShareInfoAllBot()
			webhookShareInfoBot( 
				"<a:toa:997296837352505415> **|** Your world total in the rotation world list is " .. TotalRotationWorld .. ", so the script at this index will stop running."
			)
			bot:stopScript()
			removeBot(bot.name)
			sleep(1000)
		end
		nextWorldStatus = true
	elseif nextWorldStatus == "otherCondition" and noStopScript == true then
		table.remove(Rotation_World, rotationWorldStartNumber)
		TotalRotationWorld = #Rotation_World
		rotationWorldStartNumber = rotationWorldStartNumber - 1
		if TotalRotationWorld == 0 and noStopScript == true then
			autoRecon = false
			noStopScript = false
			bot:disconnect()
			sleep(100)
			webhookShareInfoAllBot()
			webhookShareInfoBot( 
				"<a:toa:997296837352505415> **|** Your world total in the rotation world list is " .. TotalRotationWorld .. ", so the script at this index will stop running."
			)
			bot:stopScript()
			removeBot(bot.name)
			sleep(1000)
		end
		nextWorldStatus = true
	end
	
	if nextWorldStatus == true and noStopScript == true then
		rotationWorldStartNumber = rotationWorldStartNumber + 1
		if rotationWorldStartNumber <= TotalRotationWorld then
			webhookShareInfoBot(
				"**[ BOT INFORMATION ]** \n" ..
				"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
				":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
				"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
				"<:Level:1190699579230916650> **|** Bot Level: " .. bot.level .. " \n" .. 
				"<:Purple_Gem:1190695162549108876> **|** Gems Amount: " .. bot.gem_count .. " \n\n" ..
				
				"**[ WORLD INFORMATION ]** \n" ..
				"<:Globe:1190695520210006036> **|** Current Farm World: `" .. Rotation_World[rotationWorldStartNumber]:upper() .. "` \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[Farm World " .. rotationWorldStartNumber .. " of " .. TotalRotationWorld .. " World]` \n" ..
				"<:Globe:1190695520210006036> **|** PNB World: `" .. breakOtherWorldStatus() .. "` \n" ..
				"<:Globe:1190695520210006036> **|** Seed World: `" .. Save_Seed_World .. "` \n" ..
				"<:Globe:1190695520210006036> **|** Pack World: `" .. Save_Pack_World .. "` \n\n" ..
				
				"**[ TREE INFORMATION ]** \n" ..
				"<:Laser_Grid_Tree:1190695145188884661> **|** Farmed Tree: " .. getItemName(Block_ID) .. " \n\n" ..
				
				"**[ DROPPED ITEM INFORMATION ]** \n" ..
				"<:Seed_And_Block:1190695174993608805> **|** Total Seed and Block Dropped: \n" ..
				getItemName(Seed_ID) .. ": " .. dropSeedCount .. " \n" ..
				getItemName(Block_ID) .. ": " .. dropBlockCount .. " \n\n" ..
				":package: **|** Total Pack Dropped:" ..
				dropPackCount .. " \n\n" ..
				
				"**[ TIME INFORMATION ]** \n" ..
				"<:Luxurious_Wall_Clock:1190695156333150258> **|** Online Time: " .. secondON(ge)
			)
			main()
		else
			rotationWorldStartNumber = rotationWorldStartNumber - 1
			dropExceedPackGoOffline()
			
			while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
				seedWorld()
				dropSeed()
				while inventory:getItemCount(Block_ID) > 0 do
					dropBlock()
				end
				hiddenWorld()
			end
			
			while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
				pickaxeWorld()
				while not bot:isInTile(44, 13) do
					findpath(44, 13)
				end
				dropItem(Pickaxe_ID)
				if inventory:getItemCount(Pickaxe_ID) == 0 then
					webhookShareInfoDropped(
						"**[ BOT INFORMATION ]** \n" ..
						"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
						":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
						"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
						"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
						"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
						
						"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
						"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
						":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
						getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
					)
				end
				hiddenWorld()
			end
			
			dropEquipmentBeforeOfffline()
			
			autoRecon = false
			noStopScript = false
			bot:disconnect()
			sleep(100)
			webhookShareInfoAllBot()
			webhookShareInfoBot(
				"**[ BOT INFORMATION ]** \n" ..
				"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
				":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
				"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
				"<a:toa:997296837352505415> **|** Bot Information: Rotation Complete, Bot Offline!"
			)
			bot:stopScript()
			removeBot(bot.name)
			sleep(1000)
		end
	end
end
---- [ END LOOPING WORLD ] ----


---- [ CHECK RUNNING ] ----
function lastCheck()
	if bot.status == 1 then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
		dropExceedPack()
		
		if bot.status == 1 and Auto_Buy_Pack == "YES" and bot.gem_count >= Pack_Price then
			reconnect(Save_Pack_World, 1, 23)
			buyPack()
		end
		
		timeEnd()
		loopStatus()
	else
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
		lastCheck()
	end
end

function lastCheck1()
	if bot.status == 1 then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
		
		if emptyTileScan() == remainingTile then 
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
			lastCheck()
		else
			while emptyTileScan() > remainingTile do
				reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
				if inventory:getItemCount(Seed_ID) == 0 then
					takeSeed()
				end
				if Auto_Plant == "YES" then
					plantLast()
				elseif Auto_Plant == "NO" then
					lastCheck()
				end
			end
		end
	else
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
		lastCheck1()
	end
end

function lastCheck2()
	if bot.status == 1 then
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)		
		if readyTreeScan() == 0 and emptyTileScan() == remainingTile and bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and bot.level >= Min_Level then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
			pnbLast()
		elseif readyTreeScan() == 0 and emptyTileScan() == remainingTile and bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and bot.level < Min_Level then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
			lastCheck()
		elseif readyTreeScan() == 0 and emptyTileScan() ~= remainingTile and bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) and (bot.level >= Min_Level or bot.level < Min_Level) then
			reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
			lastCheck()
		else
			while readyTreeScan() > 0 and bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) do
				reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
				if inventory:getItemCount(Seed_ID) > Max_Seed_To_Drop and bot:isInWorld(Rotation_World[rotationWorldStartNumber]:upper()) then
					reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
					bot.auto_collect = false
					sleep(100)
					seedWorld()
					dropSeed()
					takeItem(Block_ID)
					hiddenWorld()
					farmWorld()
					reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
				end
				reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
				findpathBlock()
				harvest1()
			end
		end
	else
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
		lastCheck2()
	end
end
---- [ END CHECK RUNNING ] ----


---- [ ADD BOT FOR FIRST TIME ] ----
function addBotFirst()
	noStopScript = true
	autoRecon = true
	
	if Bot_Format == "CID" then
		bot:updateBot(Bot_List[botStartNumber][1], Bot_List[botStartNumber][2], randomMAC(), randomRID())
		sleep(100)
	elseif Bot_Format == "GUEST" then
		bot:updateBot(randomGrowID(), Bot_List[botStartNumber][1], randomRID())
		sleep(100)
	elseif Bot_Format == "CREATEGUEST" then
		bot:updateBot(randomGrowID(), randomMAC(), randomRID())
		sleep(100)
		webhookShareInfoBot(
			"<a:toa:997296837352505415> **|** Creating new guest, please wait. .. "
		)
		newGuest()
	elseif Bot_Format == "UBI" then
		bot:updateUbiBot(Bot_List[botStartNumber][1], Bot_List[botStartNumber][2])
		sleep(100)
	end
	
	BotName = bot.name:gsub("`.",''):upper()
	
	if bot.status ~= 1 then
		bot:connect()
		--addEvent(Event.variantlist, tutorialQuestEvent)
		--listenEvents(Reconnect_Delay_Cooldown_Seconds)
		sleep(Reconnect_Delay)
	end
	
	while bot.status ~= 1 and noStopScript == true do
		maxReconnect()
		bot:connect()
		--addEvent(Event.variantlist, tutorialQuestEvent)
		--listenEvents(Reconnect_Delay_Cooldown_Seconds)
		sleep(Reconnect_Delay)
	end
	connectCount = 0
	changingSubserver()
	if bot.status == 1 and string.find(world.name:upper(), bot.name:upper()) and bot.level < 2 then
		skipTutorial()
	end
	if Change_Bot_On_Max_Level == "YES" then
		if bot.status == 1 and bot.level >= Max_Level then
			dropExceedPackGoOffline()
			
			while (inventory:getItemCount(Seed_ID) > 0 or inventory:getItemCount(Block_ID) > 0) do
				seedWorld()
				dropSeed()
				while inventory:getItemCount(Block_ID) > 0 do
					dropBlock()
				end
				hiddenWorld()
			end
			
			while inventory:getItemCount(Pickaxe_ID) > 0 and Take_Pickaxe == "YES" do
				pickaxeWorld()
				while not bot:isInTile(44, 13) do
					findpath(44, 13)
				end
				dropItem(Pickaxe_ID)
				if inventory:getItemCount(Pickaxe_ID) == 0 then
					webhookShareInfoDropped(
						"**[ BOT INFORMATION ]** \n" ..
						"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
						":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
						"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
						"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
						"<a:toa:997296837352505415> **|** Bot Information: Dropping " .. getItemName(Pickaxe_ID) .. " \n\n" ..
						
						"**[" .. getItemName(Pickaxe_ID):upper() .. " INFORMATION ]** \n" ..
						"<:Globe:1190695520210006036> **|** " .. getItemName(Pickaxe_ID) .. " World: `" .. world.name:upper() .. "` \n" ..
						":package: **|** Total " .. getItemName(Pickaxe_ID) .. " Dropped: \n" ..
						getItemName(Pickaxe_ID) .. ": " .. objectGrowScan(Pickaxe_ID)
					)
				end
				hiddenWorld()
			end
			
			dropEquipmentBeforeOfffline()
			
			autoRecon = false
			bot:disconnect()
			sleep(100)
			webhookShareInfoAllBot()
			webhookShareInfoBot(
				"**[ BOT INFORMATION ]** \n" ..
				"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
				":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
				"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
				"<:Right_Arrow_V2:1190695167078957056>  `[Max Level]` \n" .. 
				"<:Level:1190699579230916650> **|** Bot Level: " .. bot.level
			)
			removeBotLogFile(BotName, "Max Level")
			if Change_Bot == "YES" then
				table.remove(Bot_List, botStartNumber)
				TotalBotList = #Bot_List
				botStartNumber = botStartNumber - 1
				changeBotStatus()
			elseif Change_Bot == "NO" then
				noStopScript = false
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			end
		end
	end
	randomWorldBreak()
	randomWorldQuest()
	
	BotName = bot.name:gsub("`.",''):upper()
	g = os.time()
	ge = os.time() - g
	webhookShareInfoBot(
		"**[ BOT INFORMATION ]** \n" ..
		"<:GT_Bot:1190695123336560781> **|** Bot Name: " .. BotName .. " \n" ..
		":1234: **|** Bot MAC: ||" .. bot:getLogin().mac .. "|| [" .. Bot_Format .. "] \n" ..
		"<:Status:1190695183440945182> **|** Bot Status: " .. activeAndNonActiveStatus() .. " [" .. bot:getPing() .. "] \n" ..
		"<:Right_Arrow_V2:1190695167078957056>  `[" .. statusBotDescription() .. "]` \n" ..
		"<:Level:1190699579230916650> **|** Bot Level: " .. bot.level .. " \n" .. 
		"<:Purple_Gem:1190695162549108876> **|** Gems Amount: " .. bot.gem_count .. " \n\n" ..
		
		"**[ WORLD INFORMATION ]** \n" ..
		"<:Globe:1190695520210006036> **|** Current Farm World: `" .. Rotation_World[rotationWorldStartNumber]:upper() .. "` \n" ..
		"<:Right_Arrow_V2:1190695167078957056>  `[Farm World " .. rotationWorldStartNumber .. " of " .. TotalRotationWorld .. " World]` \n" ..
		"<:Globe:1190695520210006036> **|** PNB World: `" .. breakOtherWorldStatus() .. "` \n" ..
		"<:Globe:1190695520210006036> **|** Seed World: `" .. Save_Seed_World .. "` \n" ..
		"<:Globe:1190695520210006036> **|** Pack World: `" .. Save_Pack_World .. "` \n\n" ..
		
		"**[ TREE INFORMATION ]** \n" ..
		"<:Laser_Grid_Tree:1190695145188884661> **|** Farmed Tree: " .. getItemName(Block_ID) .. " \n\n" ..
		
		"**[ DROPPED ITEM INFORMATION ]** \n" ..
		"<:Seed_And_Block:1190695174993608805> **|** Total Seed and Block Dropped: \n" ..
		getItemName(Seed_ID) .. ": " .. dropSeedCount .. " \n" ..
		getItemName(Block_ID) .. ": " .. dropBlockCount .. " \n\n" ..
		":package: **|** Total Pack Dropped:" ..
		dropPackCount .. " \n\n" ..
				
		"**[ TIME INFORMATION ]** \n" ..
		"<:Luxurious_Wall_Clock:1190695156333150258> **|** Online Time: " .. secondON(ge)
	)
	
	if Min_Level < getInfo(Block_ID).level then
		Bot_Level_Rotation = true
		Min_Level = getInfo(Block_ID).level
		webhookShareInfoBot(
			"<a:toa:997296837352505415> **|** You need to be level " .. Min_Level .. " to break " .. getItemName(Block_ID) .. "! The minimum and maximum level on " .. BotName .. " have been set to the latest. Now the minimum level is " .. Min_Level .. " and the maximum level is " .. Max_Level .. "." 
		)
	end
	
	--questSequenceBackup = false
	--startTutorialQuest()
	main()
end
---- [ END ADD BOT FOR FIRST TIME ] ----


---- [ MAIN CHECK ] ----
function main()
	reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
	
	webhookShareInfoAllBot()
	
	leveling()
	takePickaxe()
	farmWorld()
	trashItem()
	killingGhost()
	if Auto_Kill_Ghost_Only == "YES" then
		loopStatus()
	end
	antiToxic()
	antiFire()
	if Auto_Fossil == "YES" and fossilRockScan() > 0 then
		sendWebhookAmountAutoFossil = 0
		takeFossil()
	end
	
	timeStart()
	
	if readyMatureRootsScan() > 0 then
		punchMatureRoots1()
		reconnect(Rotation_World[rotationWorldStartNumber]:upper(), 1, 23)
	end
	
	if readyTreeScan() == 0 and objectSafeScan(Block_ID) > 0 and bot.level >= Min_Level and Auto_Take_Floating_Block == "YES" then
		findpathBlock()
		harvest1()
	else
		lastCheck2()
	end
end
---- [ END MAIN CHECK ] ----


---- [ VALID GROWID CHECK ] ----
function checkValidRunning()
	local checkValidRunning = true
	local checkValidRunningValid = false
	
	if checkValidRunning then
		local isNotValidGrowID = false
		for _, botlist in pairs(Bot_List) do
			local botString = botlist[1]

			if Bot_Format == "CID" and (string.find(botString, ":", 1, true) or string.find(botString, "@", 1, true)) then
				isNotValidGrowID = true
			elseif Bot_Format == "GUEST" and not string.find(botString, ":", 1, true) then
				isNotValidGrowID = true
			elseif Bot_Format == "UBI" and not string.find(botString, "@", 1, true) then
				isNotValidGrowID = true
			end
		end
		
		if isNotValidGrowID then
			webhookShareInfoBot(
				'The writing in your bot list is incorrect, the script cannot be executed further\n\n' ..
				
				'## BOT LIST FORMAT SINGLE BOT \n' ..
				'```\n' ..
				'CID account: \n' ..
				'Bot_List = { \n' ..
				'  {"GROWID1", "PASSWORD"} --Running bot \n' ..
				'} \n\n' ..
				
				'Guest account: \n' ..
				'Bot_List = { \n' ..
				'  {"MAC1"} --Running bot \n' ..
				'} \n\n' ..
				
				'Ubisoft Connect account: \n' ..
				'Bot_List = { \n' ..
				'  {"EMAIL1", "PASSWORD"} --Running bot \n' ..
				'} \n' ..
				'```\n' ..
				
				'## BOT LIST FORMAT WITH BACKUP BOT \n' ..
				'```\n' ..
				'CID account: \n' ..
				'Bot_List = { \n' ..
				'  {"GROWID1", "PASSWORD"} --Running bot \n' ..
				'  {"GROWID2", "PASSWORD"} --Running bot \n' ..
				'  {"GROWID3", "PASSWORD"} --Running bot \n' ..
				'} \n\n' ..
				
				'Guest account: \n' ..
				'Bot_List = { \n' ..
				'  {"MAC1"} --Running bot \n' ..
				'  {"MAC2"} --Running bot \n' ..
				'  {"MAC3"} --Running bot \n' ..
				'} \n\n' ..
				
				'Ubisoft Connect account: \n' ..
				'Bot_List = { \n' ..
				'  {"EMAIL1", "PASSWORD"} --Running bot \n' ..
				'  {"EMAIL2", "PASSWORD"} --Running bot \n' ..
				'  {"EMAIL3", "PASSWORD"} --Running bot \n' ..
				'} \n' ..
				'```'
			)
			bot:stopScript()
			removeBot(bot.name)
			sleep(1000)
		end
		
		if Auto_Consume == "YES" then
			local isValidAutoConsume = (Only_Consume_At_Specific_Level == "YES" and 1 or 0) +
									   (Only_Consume_Until_Minimum_Level == "YES" and 1 or 0)
			if isValidAutoConsume > 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the auto consume options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			elseif isValidAutoConsume < 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the auto consume options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			end
		end
		
		if Leveling_Status == "COOK" then
			local isValidAutoCook = (Only_Cook_At_Specific_Level == "YES" and 1 or 0) +
									   (Only_Cook_Until_Minimum_Level == "YES" and 1 or 0)
			if isValidAutoCook > 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the auto cook options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			elseif isValidAutoCook < 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the auto cook options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			end
		end
		
		if Ban_Wave_Estimated == "YES" then
			local isValidBanWaveEstimated = (Ban_Wave_Estimated_With_Online_Player == "YES" and 1 or 0) +
									   (Ban_Wave_Estimated_With_Time == "YES" and 1 or 0)
			if isValidAutoConsume > 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the ban wave estimated options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			elseif isValidAutoConsume < 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the ban wave estimated options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			end
		end
		
		if Break_Other_World == "YES" then
			local isValidBreakOtherWorld  = (Use_Custom_World_For_Break == "YES" and 1 or 0) +
											(Use_Random_World_For_Break_Other_Break == "YES" and 1 or 0) +
											(Use_Tutorial_World_For_Break == "YES" and 1 or 0)
			if isValidBreakOtherWorld > 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the break other world options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			elseif isValidBreakOtherWorld < 1 then
				webhookShareInfoBot(
					"<a:toa:997296837352505415> **|** Please choose one of the break other world options!"
				)
				bot:stopScript()
				removeBot(bot.name)
				sleep(1000)
			end
		end		
		
		checkValidRunningValid = true
		if checkValidRunningValid then
			webhookShareInfoAds(":pushpin: **|** Buy other scripts only at Grow King Scripts. \nReport bugs [click here!](<https://discord.gg/tTQ6s9pyVx>)")
			bot:getLog():append("............................................................... \n[GROW KING SCRIPTS] Script Started! \n\nBuy other scripts only at Grow King Scripts. \nReport bugs [click here!](<https://discord.gg/tTQ6s9pyVx>) \n...............................................................")
			sleep(100)
			addBotFirst()
		end
	end
end
---- [ END VALID GROWID CHECK ] ----

bot.auto_tutorial = false
bot.auto_trash = false
bot.legit_mode = showPunchAnimation()
bot.ignore_gems = ignoreGemsStatus()
if Platform_Used == "ANDORID" then
	bot:setPlatform(Platform.android)
elseif Platform_Used == "WINDOWS" then
	bot:setPlatform(Platform.windows)
elseif Platform_Used == "MACOS" then
	bot:setPlatform(Platform.macos)
end
sleep(1000)
checkValidRunning()
]]

---- [ GKS VARIABLE ] ----
--Folder and file variable
--Server data proxies setting
	Add_Custom_Http_Proxy = Add_Custom_Http_Proxy:upper()
	Custom_Http_Proxy_Rotate = Custom_Http_Proxy_Rotate:upper()
	Custom_Http_Proxy_Type = Custom_Http_Proxy_Type:upper()
	
--SOCKS5 proxies setting
	Add_Proxy = Add_Proxy:upper()
	Proxy_Type = Proxy_Type:upper()
	Proxy_Manager = getProxyManager()
--------------------------
--Folder and file variable
	logFolder = "GKSlog"
	proxyFile = logFolder .. "/proxyLog"
	httpProxyFile = logFolder .. "/httpProxyLog"
---- [ END GKS VARIABLE ] ----


---- [ MAKING GKS LOG FOLDER ] ----
function createLogFolder()
	os.execute("mkdir " .. logFolder)
end
---- [ END MAKING GKS LOG FOLDER ] ----


---- [ ADD SERVER DATA PROXIES PROXIES ] ----
function httpProxyLog()
	local filename = string.format("%s.txt", httpProxyFile)
	local newFile = io.open(filename, "w")
	for _, writeProxy in pairs(Custom_Http_Proxy_List) do
		newFile:write(writeProxy .. " \n")
		sleep(100)
	end
	newFile:close()
end

function httpProxyRotate()
	if Custom_Http_Proxy_Rotate == "YES" then
		Proxy_Manager.use_rotate = true
	elseif Custom_Http_Proxy_Rotate == "NO" then
		Proxy_Manager.use_rotate = false
	end
end

function addHttpProxyMain()
	if Add_Custom_Http_Proxy == "NO" then
		Proxy_Manager.custom_http_proxy = false
		Proxy_Manager.use_main_ip = true
	elseif Add_Custom_Http_Proxy == "YES" then
		Proxy_Manager.custom_http_proxy = true
		Proxy_Manager.use_main_ip = false
		httpProxyRotate()
		Proxy_Manager.http_cooldown = 5
		httpProxyLog()
		
		for _, getProxy in pairs(Custom_Http_Proxy_List) do
			if Custom_Http_Proxy_Type == "HTTP" then
				addDataProxy(getProxy, Proxy.http)
				sleep(100)
			elseif Custom_Http_Proxy_Type == "HTTPS" then
				addDataProxy(getProxy, Proxy.https)
				sleep(100)
			elseif Custom_Http_Proxy_Type == "SOCKS5" then
				addDataProxy(getProxy, Proxy.socks5)
				sleep(100)
			end
		end
	end
end
---- [ END ADD SERVER DATA PROXIES PROXIES ] ----


---- [ ADD SOCKS5 PROXIES ] ----
function proxyLog()
	local filename = string.format("%s.txt", proxyFile)
	local newFile = io.open(filename, "w")
	for _, writeProxy in pairs(Proxy_List) do
		newFile:write(writeProxy .. " \n")
		sleep(100)
	end
	newFile:close()
end

function addProxyMain()
	if Add_Proxy == "NO" then 
		return 
	elseif Add_Proxy == "YES" then
		proxyLog()
		
		for _, getProxy in pairs(Proxy_List) do
			Proxy_Manager:addProxy(getProxy)
			sleep(100)
		end
	end
end
---- [ END ADD SOCKS5 PROXIES ] ----


---- [ RUNNING ] ----
Proxy_Manager.localize = true
Proxy_Manager.spread = false
Proxy_Manager:setLimit(Proxy_Limit)
createLogFolder()
addHttpProxyMain()
addProxyMain()

function howManyBotsReady()
	local howManyBots = 0
	for _, bots in pairs(getBots()) do
		howManyBots = howManyBots + 1
	end
	return howManyBots
end

local remainingBots = Number_Of_Config_Used - howManyBotsReady()

if remainingBots > 0 then
	for i = 1, remainingBots do
		addBot("" .. i .. "", "" .. i .. "")
		sleep(100)
	end
end

local indexUnRunningScript = {}
function howManyBotsUnRunning()
    for i, bot in ipairs(getBots()) do
        if not bot:isRunningScript() then
            table.insert(indexUnRunningScript, i)
        end
    end
end

howManyBotsUnRunning()
for _, index in ipairs(indexUnRunningScript) do
	getBots()[index]:runScript(Multi_Config)
	sleep(100)
end
---- [ END RUNNING ] ----
